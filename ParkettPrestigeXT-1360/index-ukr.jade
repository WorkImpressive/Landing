          a.tms-youtube-card(href="https://thomas.support/r/1721", target="_blank")
            img.tms-img-resp.tms-left-shadow(src="img/tms-section-6-img-2.jpg", alt="Насадка для паркету з натуральним кінським волосом і повстю")
